{
  "name": "react-image-crop",
  "version": "0.1.1",
  "description": "A responsive image cropping tool for React",
  "repository": "https://github.com/DominicTobias/react-image-crop",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack --progress --colors --watch --dev --config demo/webpack.config.js",
    "release-js": "babel lib/ReactCrop.jsx --out-file dist/ReactCrop.js",
    "release-css": "node-sass lib/ReactCrop.scss dist/ReactCrop.css",
    "release": "npm run release-js && npm run release-css",
    "sass-watch": "node-sass lib/ReactCrop.scss dist/ReactCrop.css --watch"
  },
  "author": "Dominic Tobias <dominic.tobias@gmail.com>",
  "keywords": [
    "react",
    "image",
    "crop",
    "react-component"
  ],
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "6.2.1",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-plugin-typecheck": "3.6.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-hmre": "1.0.1",
    "babel-preset-stage-0": "6.3.13",
    "classnames": "^2.2.3",
    "core-decorators": "^0.11.0",
    "css-loader": "^0.23.1",
    "minimist": "^1.2.0",
    "node-sass": "^3.4.2",
    "postcss-cssnext": "^2.4.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-loader": "^0.8.0",
    "react-dom": "0.14.7",
    "react-styleable": "^2.2.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.1"
  },
  "dependencies": {
    "react": "^0.14.6"
  }
}
